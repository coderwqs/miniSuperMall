<!--components/w-swiper/w-swiper.wxml-->

<swiper class="swiper swiper_size" circular autoplay interval='3000' duration='300' indicator-dots indicator-active-color='#ff5777'>

  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image class="img" src="{{item.image}}" mode="widthFix" />
    </swiper-item>
  </block>
</swiper>